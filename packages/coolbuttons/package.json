{
  "name": "coolbuttons",
  "version": "1.0.4",
  "description": "Production-ready button components. Works with React, Vue, Svelte, Vanilla JS, and any framework.",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.esm.js",
      "require": "./dist/react.js",
      "types": "./dist/react.d.ts"
    },
    "./vanilla": {
      "import": "./dist/vanilla.esm.js",
      "require": "./dist/vanilla.js",
      "types": "./dist/vanilla.d.ts"
    },
    "./styles": "./dist/styles.css",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:index && npm run build:styles",
    "build:index": "tsc src/index.ts --declaration --emitDeclarationOnly --outDir dist && node build.mjs",
    "build:styles": "cp -r ../react/dist/styles.css dist/styles.css 2>/dev/null || true",
    "dev": "tsc src/index.ts --declaration --emitDeclarationOnly --outDir dist --watch",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@coolbuttons/react": "^1.0.3",
    "@coolbuttons/vanilla": "^1.0.3"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "buttons",
    "components",
    "ui",
    "tailwindcss",
    "react",
    "vanilla",
    "javascript",
    "claymorphic",
    "neumorphic",
    "glassmorphic",
    "cyberpunk",
    "minimal",
    "animated",
    "gradient",
    "responsive",
    "production-ready",
    "open-source"
  ],
  "author": "Cool Buttons",
  "repository": {
    "type": "git",
    "url": "https://github.com/coolbuttons/coolbuttons.git",
    "directory": "packages/coolbuttons"
  },
  "bugs": {
    "url": "https://github.com/coolbuttons/coolbuttons/issues"
  },
  "homepage": "https://coolbuttons.dev",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/coolbuttons"
  }
}