{
  "name": "@coolbuttons/vanilla",
  "version": "1.0.6",
  "description": "Production-ready button components for vanilla JavaScript",
  "license": "MIT",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/coolbuttons.min.js",
